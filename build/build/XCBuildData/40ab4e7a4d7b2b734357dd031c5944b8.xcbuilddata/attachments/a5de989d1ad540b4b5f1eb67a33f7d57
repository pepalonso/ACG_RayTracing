#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build
  make -f /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build
  make -f /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build
  make -f /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build/CMakeScripts/ReRunCMake.make
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build
  make -f /Users/alexandre/Documents/Advanced_Computer_Graphics/ACG_RayTracing/build/CMakeScripts/ReRunCMake.make
fi

